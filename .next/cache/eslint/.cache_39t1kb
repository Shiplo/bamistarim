v>\n            <div className='bg-mask1 absolute top-0 left-0 w-full h-full z-40 bg-center bg-no-repeat bg-cover' style={{backgroundImage: `url(${data.bgMask.src})`}}></div>\n            <div className='bg-mask2 absolute top-0 left-0 w-full h-full z-50 opacity-70' style={{backgroundColor: \"#223A57\"}}></div>\n\n        </div>\n    </>\n  )\n}\n\nexport default LandingBg","/Users/shiplorahman/Projects/bamistarim/src/components/home/section1/index.js",["86"],[],"import React from 'react'\nimport { motion } from \"framer-motion\"\nimport { LazyLoadImage } from 'react-lazy-load-image-component';\nimport styles from \"./section1.module.scss\"\nimport arrow from '../../../../public/images/arrow-left.png';\nimport bgImage from '../../../../public/images/home-section2.jpg';\nimport buttonBg from '../../../../public/images/button-bg.png';\nimport SectionBg from '../sectionBg';\n\nfunction Section1() {\n    const bgData = {\n        \"bgImage\": bgImage\n    }\n  return (\n    <>\n        <SectionBg data={bgData} />\n        <div className={`section-wrapper relative z-20 ${styles.wrapper}`}>\n            <h2 className={styles.title}>נעים מאד,</h2>\n            <div className={styles.content} >\n                <p>אנחנו רוז ושרה. יש לנו סיפור מיוחד שאנחנו מתרגשות לספר. הוא מתחיל על האניה בדרך לארץ ישראל...</p>\n            </div>\n            <button className={`${styles.button}`}>\n                <span className={styles.icon} style={{backgroundImage: `url(${buttonBg.src})`}}>\n                    <img className={`inline-block ${styles.image}`} src={arrow.src} alt=\"<-\" />\n                </span>\n                <span className='text'>לסיפור שלנו</span>\n            </button>\n        </div>\n    </>\n  )\n}\n\nexport default Section1","/Users/shiplorahman/Projects/bamistarim/src/components/home/section2/index.js",["87"],[],"import React from 'react';\nimport Image from 'next/image';\nimport bgImage from '../../../../public/images/home-section3.jpg';\nimport SectionBg fr